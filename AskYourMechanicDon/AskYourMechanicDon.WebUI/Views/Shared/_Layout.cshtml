<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Ask Your Mechanic Don</title>
    <!-- Bootstrap -->
    <link href="~/Theme/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--plugins-->
    <link href="~/Theme/css/animate.css" rel="stylesheet">
    <link href="~/Theme/css/flexslider.css" rel="stylesheet">
    <link href="~/Theme/owl-carousel/assets/owl.carousel.css" rel="stylesheet">
    <link href="~/Theme/owl-carousel/assets/owl.theme.default.css" rel="stylesheet">
    <link href="~/Theme/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/Theme/themify-icons/themify-icons.css" rel="stylesheet">
    <link href="~/Theme/css/jasny-bootstrap.min.css" rel="stylesheet">
    <!--Revolution slider css-->
    <link href="~/Theme/rs-plugin/css/settings.css" rel="stylesheet" type="text/css" media="screen">
    <link href="~/Theme/css/rev-style.css" rel="stylesheet" type="text/css" media="screen">
    <!--cube css-->
    <link href="~/Theme/cubeportfolio/css/cubeportfolio.min.css" rel="stylesheet" type="text/css">
    <!--custom css (comile from one-page.scss)-->
    <link href="~/Theme/css/one-page-dark.css" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body data-spy="scroll">
    <!--Home section start-->
    <section id="slider" class=" clearfix">
        <!--
        #################################
            - THEMEPUNCH BANNER -
        #################################
        -->
        <div class="fullwidthbanner">
            <div class="tp-fullscreen-banner">
                <ul>
                    <!-- SLIDE  -->
                    <li class="dark" data-transition="fade" data-slotamount="1" data-masterspeed="1500" data-thumb="img/showcase-4.jpg" data-delay="10000" data-saveperformance="off" data-title="Ask Your Mechanic Don">
                        <!-- MAIN IMAGE -->
                        <img src="~/Theme/img/showcase-4.jpg" alt="kenburns1" data-bgposition="left center" data-kenburns="on" data-duration="10000" data-ease="Linear.easeNone" data-bgfit="130" data-bgfitend="100" data-bgpositionend="right center">
                        <!-- LAYERS -->
                        <!-- LAYER NR. 2 -->
                        <div class="tp-caption title customin ltl tp-resizeme"
                             data-x="center"
                             data-y="200"
                             data-customin="x:0;y:150;z:0;rotationZ:0;scaleX:1;scaleY:1;skewX:0;skewY:0;opacity:0;transformPerspective:200;transformOrigin:50% 0%;"
                             data-speed="800"
                             data-start="1000"
                             data-easing="easeOutQuad"
                             data-splitin="none"
                             data-splitout="none"
                             data-elementdelay="0.01"
                             data-endelementdelay="0.1"
                             data-endspeed="1000"
                             data-endeasing="Power4.easeIn" style="z-index: 3;">
                            Ask your Mechanic Don
                        </div>
                        <div class="tp-caption customin subtitle ltl tp-resizeme"
                             data-x="center"
                             data-y="280"
                             data-customin="x:0;y:150;z:0;rotationZ:0;scaleX:1;scaleY:1;skewX:0;skewY:0;opacity:0;transformPerspective:200;transformOrigin:50% 0%;"
                             data-speed="800"
                             data-start="1250"
                             data-easing="easeOutQuad"
                             data-splitin="none"
                             data-splitout="none"
                             data-elementdelay="0.01"
                             data-endelementdelay="0.1"
                             data-endspeed="1000"
                             data-endeasing="Power4.easeIn" style="z-index: 3;">
                            <p class="lead">We simply answer your automotive question.</p>
                        </div>
                        <div class="tp-caption customin scroll-to ltl tp-resizeme"
                             data-x="center"
                             data-y="350"
                             data-customin="x:0;y:150;z:0;rotationZ:0;scaleX:1;scaleY:1;skewX:0;skewY:0;opacity:0;transformPerspective:200;transformOrigin:50% 0%;"
                             data-speed="800"
                             data-start="1550"
                             data-easing="easeOutQuad"
                             data-splitin="none"
                             data-splitout="none"
                             data-elementdelay="0.01"
                             data-endelementdelay="0.1"
                             data-endspeed="1000"
                             data-endeasing="Power4.easeIn" style="z-index: 3;"><a href="#work" class="btn btn-lg btn-white-border"><span>Ask</span> <i class="icon-angle-right"></i></a></div>
                    </li>

                    <!-- SLIDE  -->
                    <li data-transition="slideup" data-slotamount="1" data-masterspeed="1500" data-thumb="~/Theme/img/bg-4.jpg" data-delay="10000" data-saveperformance="off" data-title="Over 35 years of automotive experiance">
                        <!-- MAIN IMAGE -->
                        <img src="~/Theme/img/bg-4.jpg" alt="kenburns6" data-bgposition="center bottom" data-kenburns="on" data-duration="10000" data-ease="Linear.easeNone" data-bgfit="100" data-bgfitend="120" data-bgpositionend="center top">
                        <!-- LAYERS -->
                        <!-- LAYER NR. 2 -->
                        <div class="tp-caption title customin ltl tp-resizeme"
                             data-x="center"
                             data-y="200"
                             data-customin="x:0;y:150;z:0;rotationZ:0;scaleX:1;scaleY:1;skewX:0;skewY:0;opacity:0;transformPerspective:200;transformOrigin:50% 0%;"
                             data-speed="800"
                             data-start="1000"
                             data-easing="easeOutQuad"
                             data-splitin="none"
                             data-splitout="none"
                             data-elementdelay="0.01"
                             data-endelementdelay="0.1"
                             data-endspeed="1000"
                             data-endeasing="Power4.easeIn" style="z-index: 3;">
                            Over 35 years of automotive experiance
                        </div>
                        <div class="tp-caption subtitle customin ltl tp-resizeme"
                             data-x="center"
                             data-y="280"
                             data-customin="x:0;y:150;z:0;rotationZ:0;scaleX:1;scaleY:1;skewX:0;skewY:0;opacity:0;transformPerspective:200;transformOrigin:50% 0%;"
                             data-speed="800"
                             data-start="1250"
                             data-easing="easeOutQuad"
                             data-splitin="none"
                             data-splitout="none"
                             data-elementdelay="0.01"
                             data-endelementdelay="0.1"
                             data-endspeed="1000"
                             data-endeasing="Power4.easeIn" style="z-index: 3;">
                            <p class="lead">We provide amazingly simple answers.</p>
                        </div>
                        <div class="tp-caption customin scroll-to ltl tp-resizeme"
                             data-x="center"
                             data-y="350"
                             data-customin="x:0;y:150;z:0;rotationZ:0;scaleX:1;scaleY:1;skewX:0;skewY:0;opacity:0;transformPerspective:200;transformOrigin:50% 0%;"
                             data-speed="800"
                             data-start="1550"
                             data-easing="easeOutQuad"
                             data-splitin="none"
                             data-splitout="none"
                             data-elementdelay="0.01"
                             data-endelementdelay="0.1"
                             data-endspeed="1000"
                             data-endeasing="Power4.easeIn" style="z-index: 3;"><a href="#work" class="btn btn-lg btn-white-border"><span>Ask</span> <i class="icon-angle-right"></i></a></div>
                    </li>
                </ul>
            </div>
        </div>

        <!-- END REVOLUTION SLIDER -->
    </section>

    <!--Home section end-->
    <!--Main nav begin-->
    <nav class="navbar navbar-inverse navbar-static-top sticky">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#"><img src="" alt=""></a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
                <ul class="nav navbar-nav navbar-right scroll-to">
                    <li>@Html.ActionLink("Shop", "Index", "Home")</li>
                    <li>@Html.ActionLink("Product Admin", "Index", "ProductManager")</li>
                    <li>@Html.ActionLink("Category Admin", "Index", "ProductCategoryManager")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Disclaimer", "Disclaimer", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    @Html.Partial("_LoginPartial")
                    @{
                         
                    }
                </ul>
            </div><!--/.nav-collapse -->
            <button type="button" class="navbar-toggle side-panel" data-toggle="offcanvas" data-target=".navmenu" data-canvas="body"><i class="ti-menu-alt"></i></button>
        </div><!--/.container -->
    </nav>
    <!--Main nav end-->
    <section id="work">
        <div class="container body-content">
            @RenderBody()
            <hr />
        </div>
    </section>
    <!--footer-->
    <footer class="footer">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3 text-center margin20">
                    <div class="footer-col">
                        @*<h3><img src="~/Theme/img/logo-white.png" alt=""></h3>*@
                        <div class="social">
                            <a href="#" class="social-icon si-dark si-gray-round si-colored-facebook">
                                <i class="fa fa-facebook"></i>
                                <i class="fa fa-facebook"></i>
                            </a>
                            <a href="#" class="social-icon si-dark si-gray-round si-colored-google-plus">
                                <i class="fa fa-google-plus"></i>
                                <i class="fa fa-google-plus"></i>
                            </a>
                            <a href="#" class="social-icon si-dark si-gray-round si-colored-instagram">
                                <i class="fa fa-instagram"></i>
                                <i class="fa fa-instagram"></i>
                            </a>
                            <a href="#" class="social-icon si-dark si-gray-round si-colored-linkedin">
                                <i class="fa fa-linkedin"></i>
                                <i class="fa fa-linkedin"></i>
                            </a>
                        </div>
                    </div>
                </div><!--footer col-->
            </div>
            <div class="row">
                <div class="col-md-12 text-center">
                    <div class="footer-btm">
                        <span>&copy;2017. Theme by Design_mylife</span>
                    </div>
                </div>
            </div>
        </div>
    </footer><!--footer 2 end-->
    <!--footer end-->
    <div id="back-to-top">
        <a href="#top"><i class="fa fa-angle-up"></i></a>
    </div>
    <!-- jQuery-->
    <script src="~/Theme/js/jquery.min.js"></script>
    <script src="~/Theme/js/jquery-migrate.min.js"></script>
    <script src="~/Theme/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Theme/js/jquery.easing.1.3.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/wow.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/jquery.sticky.js" type="text/javascript"></script>
    <script src="~/Theme/js/waypoints.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/jquery.counterup.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/modernizr.custom.97074.js" type="text/javascript"></script>
    <script src="~/Theme/js/parallax.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/jquery.flexslider-min.js" type="text/javascript"></script>
    <script src="~/Theme/owl-carousel/owl.carousel.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/one-page.js" type="text/javascript"></script>
    <!--cube portfolio plugin-->
    @*<script src="~/Theme/cubeportfolio/js/jquery.cubeportfolio.min.js" type="text/javascript"></script>
        <script src="~/Theme/js/cube-portfolio.js" type="text/javascript"></script>*@
    <!--cantact form script-->
    @*<script src="~/Theme/js/contact_me.js" type="text/javascript"></script>
        <script src="~/Theme/js/jqBootstrapValidation.js" type="text/javascript"></script>
        <script type="text/javascript" src="~/Theme/js/jasny-bootstrap.min.js"></script>
        <script type="text/javascript" src="~/Theme/js/pace.min.js"></script>*@
    <!--revolution slider plugins-->
    <script src="~/Theme/rs-plugin/js/jquery.themepunch.tools.min.js" type="text/javascript"></script>
    <script src="~/Theme/rs-plugin/js/jquery.themepunch.revolution.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Theme/js/revolution-custom.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>



